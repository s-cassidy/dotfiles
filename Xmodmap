!! To run this file, execute "xmodmap .Xmodmap"

!! No Caps Lock
clear lock
!! Make Caps_lock an escape key.
keycode 0x42 = Escape

!! Swap semicolon and blackslash
keycode 47 = backslash colon semicolon colon dead_acute dead_doubleacute dead_acute
keycode  94 = semicolon bar backslash bar bar brokenbar bar

!! Swap @ and "
keycode 11 = 2 quotedbl
keycode 48 = apostrophe at


